<section class="table1 section-table cid-rJfBbhIXcd" id="table1-9">
  <div class="container-fluid" style="padding-bottom: 100px;">
    <div class="media-container-row align-center">
      <div class="col-12 col-md-12">
        <div class="table-wrapper" style="width: 88%;">
          <div class="container-fluid">
          </div>
          <div class="container-fluid scroll">
            <table class="table table-striped" cellspacing="0">
              <thead>
                <tr class="table-heads">
                  <th class="head-item mbr-fonts-style display-4">#</th>
                  <th class="head-item mbr-fonts-style display-4">Product Name</th>
                  <th class="head-item mbr-fonts-style display-4">Price</th>
                  <th class="head-item mbr-fonts-style display-4">Rent Price</th>
                </tr>
              </thead>
              <tbody>
                {{#each rows}}
                <tr>
                  <td><center>{{this.id}}</center></td>
                  <td>{{this.pname}}</td>
                  <td><center>${{this.buy}}.00</center><br>
                  </td>
                  <td>
                    {{#if this.rent}}
                    <center>${{this.rent}}.00/day</center><br>
                    {{else}}
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    {{/if}}
                  </td>
                </tr>
                {{/each}}
              </tbody>
            </table>
          </div>
          <div class="container-fluid table-info-container">
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
