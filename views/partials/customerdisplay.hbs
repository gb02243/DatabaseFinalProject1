<section class="table1 section-table cid-rJfBbhIXcd mbr-fullscreen" id="table1-9">
  <div class="container-fluid">
    <div class="media-container-row align-center">
      <div class="col-12 col-md-12">
        <div class="table-wrapper" style="width: 88%;">
          <div class="container-fluid">
          </div>
          <div class="container-fluid scroll">
            <table class="table table-striped" cellspacing="0">
              <thead>
                <tr class="table-heads">
                  <th class="head-item mbr-fonts-style display-4">id</th>
                  <th class="head-item mbr-fonts-style display-4">First Name</th>
                  <th class="head-item mbr-fonts-style display-4">Last Name</th>
                  <th class="head-item mbr-fonts-style display-4">Address</th>
                  <th class="head-item mbr-fonts-style display-4">Username</th>
                </tr>
              </thead>
              <tbody>
                {{#each rows}}
                <tr>
                  <td><center>{{this.id}}</center></td>
                  <td><center>{{this.fname}}</center></td>
                  <td><center>{{this.lname}}</center></td>
                  <td><center>{{this.address}}</center></td>
                  <td><center>{{this.username}}</center></td>
                </tr>
                {{/each}}
              </tbody>
            </table>
          </div>
          <div class="container-fluid table-info-container">
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
